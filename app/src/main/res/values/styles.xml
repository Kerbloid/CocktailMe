<?xml version="1.0" encoding="utf-8"?>
<resources>

    <style name="ExpandedToolbarText" parent="TextAppearance.Design.CollapsingToolbar.Expanded">
        <item name="android:textColor">?attr/colorPrimary</item>
    </style>

    <style name="CollapsedToolbarText">
        <item name="android:textColor">?attr/colorPrimary</item>
    </style>

    <style name="ChipAction" parent="Widget.MaterialComponents.Chip.Action">
        <item name="chipBackgroundColor">?android:attr/windowBackground</item>
        <item name="chipStrokeColor">?attr/colorPrimary</item>
        <item name="chipStrokeWidth">1dp</item>
        <item name="android:textColor">?attr/colorPrimary</item>
        <item name="android:checkable">false</item>
        <item name="android:clickable">false</item>
        <item name="android:focusable">false</item>
    </style>
</resources>